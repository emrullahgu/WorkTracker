{
  "buildCommand": "prisma generate && next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "crons": [{
    "path": "/api/cron/event-reminders",
    "schedule": "*/5 * * * *"
  }, {
    "path": "/api/cron/daily-task-reminder",
    "schedule": "45 13 * * *"
  }]
}
